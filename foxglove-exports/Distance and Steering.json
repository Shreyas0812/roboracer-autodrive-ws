    "323711eb-59e1-4178-9618-a3cbb842272b": {
      "sourceCode": "import { Input, Message } from \"./types\";\n\ntype Output = {\n  data: number;\n};\ntype GlobalVariables = { throttle_value: number };\n\nexport const inputs = [\"/autodrive/f1tenth_1/throttle\"];\nexport const output = \"/foxglove_script/throttle_command_bridge\";\n\nexport default function script(\n  event: any,\n  globalVars: GlobalVariables,\n): Output {\n  //log(globalVars.throttle_value);\n  return {\n    data: globalVars.throttle_value,\n  };\n}\n",
      "name": "publish_throttle"
    },
    "2eb31fc9-f9f2-4c96-9026-a755a5f6442a": {
      "sourceCode": "import { Input, Message } from \"./types\";\n\ntype Output = {\n  data: number;\n};\ntype GlobalVariables = { steering_value: number };\n\nexport const inputs = [\"/autodrive/f1tenth_1/steering\"];\nexport const output = \"/foxglove_script/steering_command_bridge\";\n\nexport default function script(\n  event: any,\n  globalVars: GlobalVariables,\n): Output {\n  //log(globalVars.throttle_value);\n  return {\n    data: globalVars.steering_value,\n  };\n}\n",
      "name": "publish_steering"
    },
    "e77be2bd-1a79-4b6a-befd-fb6894b9551a": {
      "sourceCode": "import { Input, Message } from \"./types\";\n\ninterface Output {\n  cur_dist: number;\n  required_dist: number;\n  total_width: number;\n  left_wall_dist: number;\n  right_wall_dist: number;\n}\n\ninterface GlobalVariables {\n  reference_line_value: number;\n}\n\nexport const inputs = [\"/autodrive/f1tenth_1/lidar\"];\nexport const output = \"/foxglove_script/disp_dist\";\n\nexport default function script(\n  event: any,\n  globalVars: GlobalVariables,\n): Output {\n  const ranges = event.message.ranges;\n\n  const angle_min = event.message.angle_min;\n  const angle_max = event.message.angle_max;\n  const angle_increment = event.message.angle_increment;\n\n  // Averaging lidar measurement over a span of angles\n  const getAverageRange = (centerAngle: number, spanWidth: number) => {\n    const start_angle = centerAngle - spanWidth / 2;\n    const end_angle = centerAngle + spanWidth / 2;\n\n    const start_index = Math.round((start_angle - angle_min) / angle_increment);\n    const end_index = Math.round((end_angle - angle_min) / angle_increment);\n\n    let sum = 0;\n    let count = 0;\n\n    for (let i = start_index; i <= end_index; i++) {\n      if (\n        i >= 0 &&\n        i < ranges.length &&\n        ranges[i] &&\n        ranges[i] < event.message.range_max\n      ) {\n        sum += ranges[i];\n        count++;\n      }\n    }\n\n    return count > 0 ? sum / count : 0;\n  };\n\n  const left_wall_dist = getAverageRange(-Math.PI / 2, Math.PI / 7);\n  const right_wall_dist = getAverageRange(Math.PI / 2, Math.PI / 7);\n\n  const total_width = left_wall_dist + right_wall_dist;\n\n  const min_range = Math.min(...ranges);\n\n  const reference_line_value = globalVars.reference_line_value || 0;\n\n  return {\n    cur_dist: min_range,\n    required_dist: reference_line_value,\n    total_width: total_width,\n    left_wall_dist: left_wall_dist,\n    right_wall_dist: right_wall_dist,\n  };\n}\n",
      "name": "publish_min_dist"
    }
  },
